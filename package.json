{
  "name": "com.davidhellmann.nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "icons:build &concurrently \"npm run codegen --watch\" \"next dev\"",
    "build": "icons:build & next build",
    "start": "concurrently \"npm run codegen --watch\" \"next start\"",
    "lint": "eslint ./src",
    "prebuild": "npm run lint",
    "upgrade-interactive": "npm-check --update",
    "ui": "npm run upgrade-interactive",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env",
    "icons:build": "npx tsx ./build-icons.mts",
    "icons:add": "npx @sly-cli/sly add"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@apollo/experimental-nextjs-app-support": "^0.11.3",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "graphql-tag": "^2.12.6",
    "html-react-parser": "^5.1.17",
    "next": "^14.2.14",
    "react": "^18",
    "react-aria-components": "^1.4.0",
    "react-dom": "^18",
    "tailwind-variants": "^0.2.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@parcel/watcher": "^2.4.1",
    "@sly-cli/sly": "^1.14.0",
    "@types/eslint__js": "^8.42.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18",
    "concurrently": "^9.0.1",
    "eslint": "^9.12.0",
    "eslint-config-next": "^14.2.14",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "node-html-parser": "^6.1.13",
    "npm-check": "^6.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
  }
}
